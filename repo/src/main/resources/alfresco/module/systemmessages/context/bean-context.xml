<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	

  <!-- Registration of new model -->
  <bean id="rl.ldapDictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
    <property name="models">
      <list>
        <value>alfresco/module/systemmessages/context/model/systemmessages-model.xml</value>
      </list>
    </property>
  </bean>
  
   <!-- Webscript to get current system messages  -->
  <bean id="webscript.org.redpill.notifications.notifications.get" class="org.redpill.alfresco.systemmessages.webscript.SystemMessagesGet" parent="webscript">
    <property name="nodeService" ref="NodeService" />
    <property name="searchService" ref="searchService"/>
    <property name="namespaceService" ref="namespaceService"/>
  </bean>
  <bean id="webscript.org.redpill.notifications.notifications-datalist.get" class="org.redpill.alfresco.systemmessages.webscript.GetDataList" parent="webscript">
    <property name="nodeService" ref="NodeService" />
    <property name="searchService" ref="searchService"/>
    <property name="namespaceService" ref="namespaceService"/>
  </bean>



  <bean id="patch.CreateDataList" class="org.redpill.alfresco.systemmessages.patch.CreateDataList" parent="basePatch">
    <property name="id"><value>patch.CreateDataList</value></property>
    <property name="description"><value>patch.CreateDataList.description</value></property>
    <property name="fixesFromSchema"><value>0</value></property>
    <property name="fixesToSchema"><value>${version.schema}</value></property>
    <property name="targetSchema"><value>10000</value></property>
    <property name="searchService" ref="searchService"/>
    <property name="nodeService" ref="nodeService"/>
    <!--<property name="force" value="true" />-->
    <!--<property name="requiresTransaction"><value>true</value></property>-->
    <!--<property name="applyToTenants"><value>true</value></property>-->
    <!--<property name="contentService" ref="ContentService"/>-->
    <!--<property name="fileFolderService" ref="FileFolderService"/>-->
    <!--<property name="repository" ref="repositoryHelper"/>-->
  </bean>

  
</beans>
