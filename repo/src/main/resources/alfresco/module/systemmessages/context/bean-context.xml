<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	

  <!-- Registration of new model -->
  <bean id="rl.ldapDictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
    <property name="models">
      <list>
        <value>alfresco/module/systemmessages/context/model/systemmessages-model.xml</value>
      </list>
    </property>
  </bean>
  
   <!-- Webscript to get current system messages  -->
  <bean id="webscript.org.redpill.notifications.notifications.get" class="org.redpill.alfresco.systemmessages.webscript.SystemMessagesGet" parent="webscript">
    <property name="nodeService" ref="NodeService" />
  </bean>
  
</beans>
